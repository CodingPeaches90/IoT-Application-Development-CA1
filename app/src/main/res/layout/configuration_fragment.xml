<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/soundSubmitRate"
        style="@style/Widget.MaterialComponents.Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="129dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="129dp"
        android:fontFamily="@font/roboto_mono_bold"
        android:text="Submit"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/tempRate"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/tempRate2"
        android:layout_width="0dp"
        android:layout_height="39dp"
        android:layout_marginTop="59dp"
        android:layout_marginBottom="60dp"
        android:ems="10"
        android:fontFamily="@font/roboto_mono_bold"
        android:inputType="textPersonName"
        android:singleLine="true"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/soundSubmitRate"
        app:layout_constraintEnd_toEndOf="@+id/SoundStateSwitch"
        app:layout_constraintStart_toStartOf="@+id/SoundStateSwitch"
        app:layout_constraintTop_toTopOf="@+id/SoundStateSwitch"
        tools:text="Rate" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="93dp"
        android:layout_marginBottom="93dp"
        android:fontFamily="@font/roboto_mono_bold"
        android:text="Temperature Rate"
        app:layout_constraintBottom_toTopOf="@+id/divider"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="27dp"
        android:fontFamily="@font/roboto_mono_bold"
        android:text="Temperature State"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Switch
        android:id="@+id/switch1"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="21dp"
        android:fontFamily="@font/roboto_mono_bold"
        android:switchTextAppearance="@style/TextAppearance.AppCompat.Caption"
        android:text="State"
        android:textOff="OFF"
        android:textOn="ON"
        app:layout_constraintBottom_toTopOf="@+id/tempRate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Switch
        android:id="@+id/SoundStateSwitch"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="50dp"
        android:fontFamily="@font/roboto_mono_bold"
        android:switchTextAppearance="@style/TextAppearance.AppCompat.Caption"
        android:text="State"
        android:textOff="OFF"
        android:textOn="ON"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/divider" />

    <Switch
        android:id="@+id/soundStateSwitch"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="36dp"
        android:fontFamily="@font/roboto_mono_bold"
        android:switchTextAppearance="@style/TextAppearance.AppCompat.Caption"
        android:text="State"
        android:textOff="OFF"
        android:textOn="ON"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/soundSubmitRate" />

    <EditText
        android:id="@+id/tempRate"
        android:layout_width="75dp"
        android:layout_height="39dp"
        android:layout_marginTop="75dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="80dp"
        android:ems="10"
        android:fontFamily="@font/roboto_mono_bold"
        android:inputType="textPersonName"
        android:singleLine="true"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/temperatureSubmitRate"
        app:layout_constraintEnd_toEndOf="@+id/switch1"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Rate" />

    <Button
        android:id="@+id/temperatureSubmitRate"
        style="@style/Widget.MaterialComponents.Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="129dp"
        android:layout_marginEnd="129dp"
        android:layout_marginBottom="13dp"
        android:fontFamily="@font/roboto_mono_bold"
        android:text="Submit"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/divider"
        app:layout_constraintEnd_toEndOf="@+id/tempRate"
        app:layout_constraintStart_toStartOf="@+id/textView2" />

    <View
        android:id="@+id/divider"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="207dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="205dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="@+id/soundSubmitRate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="255dp"
        android:fontFamily="@font/roboto_mono_bold"
        android:text="Sound State"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="56dp"
        android:fontFamily="@font/roboto_mono_bold"
        android:text="Sound Rate"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <View
        android:id="@+id/divider2"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="62dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="53dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="@+id/soundStateSwitch"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/soundSubmitRate" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="26dp"
        android:fontFamily="@font/roboto_mono_bold"
        android:text="LED State"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider2" />

</android.support.constraint.ConstraintLayout>